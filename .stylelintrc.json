{
  "defaultSeverity": "warning",
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-sass-guidelines",
    "stylelint-prettier/recommended"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": [
        true,
        {
            "ignoreAtRules": ["tailwind"]
        }
    ]
}
}
